model Specialty {
  id              String    @id @default(uuid())
  name            String    @unique
  appointmentFee  Decimal   @db.Decimal(10, 2)
  percProfessional Decimal @db.Decimal(4, 1) @default(0)
  doctors         Doctor[]
  createdAt       DateTime  @default(now())
  updatedAt       DateTime  @updatedAt
  deletedAt       DateTime?
}

model Tenant {
  id        String    @id @default(uuid())
  name      String
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

  userTenants UserTenant[]
  doctors     Doctor[]
  appointments Appointment[]
  transactions Transaction[]
  specialties  Specialty[]

  @@index([deletedAt])
}


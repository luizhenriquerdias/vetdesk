enum UserTenantRole {
  admin
  user
  dev
}

model UserTenant {
  userId   String
  tenantId String
  role     UserTenantRole @default(user)

  user   User   @relation(fields: [userId], references: [id], onDelete: Cascade)
  tenant Tenant @relation(fields: [tenantId], references: [id], onDelete: Cascade)

  @@id([userId, tenantId])
  @@index([userId])
  @@index([tenantId])
}


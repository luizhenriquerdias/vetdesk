model User {
  id        String    @id @default(uuid())
  firstName String
  lastName  String
  email     String    @unique
  password  String
  avatarUrl String?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?

  transactionsCreated Transaction[] @relation("TransactionCreatedBy")
  transactionsUpdated Transaction[] @relation("TransactionUpdatedBy")
  transactionsDeleted Transaction[] @relation("TransactionDeletedBy")
  appointmentsCreated Appointment[] @relation("AppointmentCreatedBy")
  appointmentsUpdated Appointment[] @relation("AppointmentUpdatedBy")
  appointmentsDeleted Appointment[] @relation("AppointmentDeletedBy")
}
